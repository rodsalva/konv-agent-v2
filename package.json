{
  "name": "mercadolivre-ai-agent-analysis",
  "version": "1.0.0",
  "description": "MercadoLivre AI Agent Analysis System - Multi-persona marketplace intelligence with actionable business insights",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run build:dashboard",
    "build:typescript": "tsc",
    "build:dashboard": "echo 'Building static dashboard...' && cp admin_dashboard.html index.html && echo 'Static build completed!'",
    "build:vercel": "echo 'Building static dashboard for Vercel...' && cp admin_dashboard.html index.html && echo 'Static build completed!'",
    "build:local": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "cross-env TEST_MODE=integration jest --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:test": "tsc -p tsconfig.test.json --noEmit",
    "db:generate": "supabase gen types typescript --project-id=xtmjpsrjkudpfhakhfyv > src/types/database.types.ts",
    "db:migrate": "supabase db diff --use-migra --schema public",
    "db:check": "tsx src/scripts/db-check.ts",
    "db:setup": "supabase db push",
    "agents:list": "tsx src/scripts/list-agents.ts",
    "agents:demo": "tsx src/scripts/agent-management-demo.ts",
    "agents:redesign": "tsx src/scripts/company-agent-redesign.ts",
    "agents:create-company": "tsx src/scripts/create-company-agents.ts",
    "feedback:test": "tsx src/scripts/test-feedback.ts",
    "start:dev": "tsx src/scripts/start-server.ts",
    "agents:delete-original": "tsx src/scripts/delete-original-agents.ts",
    "mercadolivre:explore": "cd python-agents && source env/bin/activate && python mercadolivre_orchestrator.py",
    "personas:test": "tsx src/scripts/test-persona-api.ts",
    "departments:test": "tsx src/scripts/test-department-insights.ts",
    "competitors:test": "tsx src/scripts/test-competitor-benchmark.ts",
    "sentiment:test": "tsx src/scripts/test-sentiment-analysis.ts"
  },
  "keywords": [
    "mercadolivre",
    "ai-agents",
    "marketplace-analysis",
    "business-intelligence",
    "persona-simulation",
    "multi-agent-system",
    "typescript",
    "python",
    "express",
    "supabase"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.1",
    "@types/node": "^20.10.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pino": "^9.7.0",
    "pino-pretty": "^10.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "axios": "^1.9.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
